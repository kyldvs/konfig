
# easier nuclide development
alias ncd="cd $LOCAL_FBSOURCE/fbobjc/Tools/Nuclide/pkg/nuclide"
alias nbuild="(cd $LOCAL_FBSOURCE/fbobjc && ./Tools/Nuclide/scripts/dev/setup)"
alias npush="(cd $LOCAL_FBSOURCE/fbobjc/Tools/Nuclide && ./scripts/dev/fb/devserver-setup)"
alias nud="(cd $LOCAL_FBSOURCE/fbobjc/Tools/Nuclide && ./scripts/dev/fb/upload_dependencies)"
